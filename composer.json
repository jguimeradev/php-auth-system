{
    "name": "debian/auth",
    "description": "auth system",
    "type": "project",
    "require": {
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.5",
        "monolog/monolog": "^3.9",
        "robmorgan/phinx": "^0.16.10",
        "phpstan/phpstan": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Auth\\": "./"
        }
    },
    "config": {
        "process-timeout": 0
    },
    "scripts": {
        "dev" :  "php -S localhost:6969 -t public",
        "phinx":  "./vendor/bin/phinx",
        "phpunit":  "./vendor/bin/phpunit"
    },
    "authors": [
        {
            "name": "jguimeradev",
            "email": "jguimeradev@gmail.com"
        }
    ],
    "minimum-stability": "alpha"
}
